{
  "$schema": "https://turborepo.com/schema.json",
  "extends": ["//"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".nitro/**", ".output/**", ".tanstack/**"]
    },
    "build:vercel": {
      "dependsOn": ["^build:vercel"],
      "outputs": [".nitro/**", ".output/**", ".tanstack/**", ".vercel/**"]
    },
    "dev": {
      "persistent": true
    }
  }
}
