  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "start": "node .output/server/index.mjs",
    "serve": "vite preview",
    "lint": "biome lint",
    "format": "biome format --write .",
    "check": "biome check",
