    "ui": "bunx --bun shadcn@canary",
    "auth": "bunx --bun @better-auth/cli@latest",
    "auth:generate": "bunx --bun @better-auth/cli@latest generate --y --output ./src/lib/server/schema/auth.schema.ts && prettier --write ./src/lib/server/schema/auth.schema.ts",
    "prepare": "husky",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
